            "Contrast",
            "Timing"
          ]
        ],
        "is_readonly_function": "return true",
        "max": 100,
        "min": 0,
        "name": "Repetition Time",
        "step": 0.1,
        "unit": "ms",
        "unit_scaling": "1e-3",
        "value_type": "float"
      }
    },
